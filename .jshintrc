{
    "node": true,
    "esnext": true,
    "globalstrict": false,
    "strict": false,//strict declared at top
    "devel": true,

    "smarttabs": true,
    "trailing": true,
    "indent": 4,
    "quotmark": "double",

    "unused": true,
    "undef": true,
    "eqnull": true,
    "immed": true,
    "noarg": true,
    "freeze": false,
    "maxcomplexity": 10,

    "lastsemic": true,
    "laxcomma": false,

    "newcap": true,
    "nonew": false
}
